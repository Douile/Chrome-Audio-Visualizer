{
	"manifest_version": 2,
	"name": "__MSG_ext_name__",
	"version": "1.24",
	"description": "__MSG_ext_desc__",
	"default_locale": "en",
	"short_name": "AV",
	"author": "Douile",
	"offline_enabled": true,
	"icons": {
		"16": "/img/iconon16.png",
		"48": "/img/iconon48.png",
		"128": "/img/iconon128.png"
	},
	"permissions": [
		"storage",
		"tabs",
		"tabCapture",
		"activeTab",
		"contextMenus",
		"*://*/*",
		"notifications"
	],
	"content_scripts": [
		{
			"matches": ["*://*/*"],
			"js": [
				"/js/storage.js",
				"/js/visualizationsdata.js",
				"/js/inject.js"
			],
			"css": [
				"/css/inject.css"
			],
			"run_at": "document_end"
		}
	],
	"browser_action": {
		"default_icon": {
			"128": "/img/iconoff128.png"
		}
	},
	"background": {
		"scripts": ["/js/audioCapture.js","/js/background.js"],
		"persistent": true
	},
	"options_ui": {
		"chrome_style": true,
		"page": "options.html"
	},
	"web_accessible_resources": [
		"/img/settings.svg"
	]
}
